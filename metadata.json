{
  "name": "jfrog-docker-desktop-extension",
  "provider": "JFrog",
  "icon": "icon.svg",
  "ui": {
    "dashboard-tab": {
      "title": "JFrog",
      "root": "/ui",
      "src": "index.html"
    }
  },
  "host": {
    "binaries": [
      {
        "darwin": [
          {
            "path": "/binaries/darwin/jf"
          },
          {
            "path": "/binaries/unix/runcli.sh"
          },
          {
            "path": "/binaries/unix/readconf.sh"
          },
          {
            "path": "/binaries/unix/writeconf.sh"
          },
          {
            "path": "/binaries/unix/scanpermissions.sh"
          }
        ],
        "linux": [
          {
            "path": "/binaries/linux/jf"
          },
          {
            "path": "/binaries/unix/runcli.sh"
          },
          {
            "path": "/binaries/unix/readconf.sh"
          },
          {
            "path": "/binaries/unix/writeconf.sh"
          },
          {
            "path": "/binaries/unix/scanpermissions.sh"
          }
        ],
        "windows": [
          {
            "path": "/binaries/windows/jf.exe"
          },
          {
            "path": "/binaries/windows/runcli.bat"
          },
          {
            "path": "/binaries/windows/readconf.bat"
          },
          {
            "path": "/binaries/windows/writeconf.bat"
          },
          {
            "path": "/binaries/windows/iswindows.bat"
          },
          {
            "path": "/binaries/windows/scanpermissions.bat"
          }
        ]
      }
    ]
  }
}
